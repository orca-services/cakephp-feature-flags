{
    "name": "orca-services/cakephp-feature-flags",
    "description": "Feature Flags plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "cakephp/cakephp": "^4.0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "FeatureFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FeatureFlags\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
