{
    "name": "orca-services/cakephp-feature-flags",
    "description": "Feature Flags plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "FeatureFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FeatureFlags\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
